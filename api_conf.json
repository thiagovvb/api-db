{
   "/clients":{
      "table_name":"Clients",
      "fields":{
         "client_id":"integer",
         "client_name":"varchar",
         "address":"varchar",
         "email":"varchar"
      },
      "primary_keys":[
         "client_id"
      ]
   },
   "/order-header":{
      "table_name":"OrderHeader",
      "fields":{
         "order_id":"integer",
         "client_id":"integer",
         "seller_id":"integer",
         "order_date":"datetime",
         "total_amount":"float"
      },
      "primary_keys":[
         "order_id"
      ]
   },
   "/order-item":{
      "table-name":"OrderItem",
      "fields":{
         "order_item_id":"integer",
         "order_id":"integer",
         "product_id":"integer",
         "quantity":"integer",
         "item_value":"float"
      },
      "primary_keys":[
         "order_item_id"
      ]
   },
   "/product":{
      "table_name":"Products",
      "fields":{
         "product_id":"integer",
         "product_name":"varchar",
         "price":"float"
      },
      "primary_keys":[
         "product_id"
      ]
   },
   "/seller":{
      "table_name":"Sellers",
      "fields":{
         "seller_id":"integer",
         "seller_name":"varchar",
         "contact_number":"varchar"
      },
      "primary_keys":[
         "seller_id"
      ]
   }
}